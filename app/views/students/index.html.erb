<ul class="students_list">
    <%  @students.each do |student|  %>
        <li>
            <div class="name"><%=  student.first_name + " " + student.last_name  %></div>
            <div class="email"><a href="mailto:<%=  student.email  %>"><%=  student.email  %></a></div>
        </li>
    <% end %>
</ul>