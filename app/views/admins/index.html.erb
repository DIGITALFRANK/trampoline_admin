<ul class="admins_list">
    <%  @admins.each do |admin|  %>
        <li> 
            <div class="name"><%=  admin.first_name + " " + admin.last_name  %> </div>
            <div class="email"><a href="mailto:<%=  admin.email  %>"><%=  admin.email  %></a></div>
        </li>
    <% end %>
</ul>